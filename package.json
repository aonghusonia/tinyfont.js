{
  "name": "tinyfont",
  "version": "1.0.0",
  "description": "Tiniest possible pixel font for your js games (suitable for js13k)",
  "main": "dist/tinyfont.cjs.js",
  "module": "dist/tinyfont.esm.js",
  "browser": "dist/tinyfont.umd.js",
  "repository": "https://github.com/darkwebdev/tinyfont.js.git",
  "author": "Timur Manyanov <gtibalt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && yarn rollup -c",
    "min": "yarn terser dist/index.js dist/pixel.js -o dist/tinyfont.min.js --config-file .terser.json --toplevel "
  },
  "devDependencies": {
    "rollup": "^1.21.2",
    "terser": "^4.3.0"
  }
}
